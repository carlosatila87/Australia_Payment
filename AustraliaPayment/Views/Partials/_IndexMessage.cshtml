@model string
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    ViewBag.Title = "Australia Payment App";
}
<div class="row">
    <div id="payform" class="col-md-4 col-md-offset-4">
        <h2>Bank Payment Details</h2>
        <hr />
        <div class="panel">
            <div class="panel-info">
                <p class="text-success">Your transaction was sucessfully done!</p>
                <a href="/Logs/@Model" target="_blank">Download the Payment log.</a>
            </div>
        </div>
    </div>
</div>

